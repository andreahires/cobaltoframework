package com.cobalto.utils{	import com.cobalto.ApplicationFacade;		import flash.net.URLRequest;	import flash.net.navigateToURL;
		public class Web	{		public static function getURL(url:String,window:String=null,donwload:Boolean=false):void		{			var req:URLRequest;						if(url.slice(0,4) == "http")			{				req = new URLRequest(url);								try				{					navigateToURL(req,window);				}				catch(e:Error)				{					trace("Navigate to URL failed",e.message);				}			}			else			{				if(donwload == false)				{					ApplicationFacade.getInstance().sendInternalURL(url);				}				else				{					req = new URLRequest(url);					navigateToURL(req,"_blank");				}			}		}	}}